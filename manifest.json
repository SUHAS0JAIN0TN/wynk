{
    "name": "WYNK playlist",
    "version": "3.0.0",
    "description": "WYNK playlist description",
    "manifest_version": 3,
    "author": "Suhas Jain",
    "permissions": [
        "storage",
        "tabs"
      ],
    "content_scripts":[
    {
        "matches":[
        "https://wynk.in/*"
        ],
        "js": ["content.js"]
    }
    ],
    "action":{
        "default_popup": "index.html",
        "default_title": "WYNK playlist"
    }
}